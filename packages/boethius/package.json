{
  "name": "boethius",
  "version": "1.0.0",
  "description": "Music Notation",
  "main": "index.js",
  "scripts": {
    "watchify": "watchify src/Scored.js -t [babelify --blacklist regenerator] -o dist/boethius.js -v -d",
    "build": "browserify src/Scored.js -t babelify --outfile dist/boethius.js",
    "es6compile": "babel --modules umd src/ --out-dir lib/ -s",
    "es6watch": "babel --modules umd src/ --watch --out-dir lib/ -s",
    "test": "mocha --compilers js:babel/register"
  },
  "keywords": [
    "music",
    "notation",
    "score",
    "note"
  ],
  "author": "Sean Mullen <srmullen13@gmail.com> (http://seanmullen.me/)",
  "license": "Sean Mullen",
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "jquery": "^2.1.4"
  },
  "dependencies": {
    "fontloader": "^1.2.1",
    "fraction.js": "^3.0.0",
    "lodash": "^3.10.1",
    "teoria": "^1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srmullen/boethius.git"
  }
}
