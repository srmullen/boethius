{
  "name": "boethius",
  "version": "1.0.0",
  "description": "Music Notation",
  "main": "src/Scored.js",
  "scripts": {
    "watchify": "watchify src/Scored.js -t [babelify] -o dist/boethius.js -v -d",
    "build": "browserify src/Scored.js -t babelify --outfile dist/boethius.js",
    "es6compile": "babel --modules umd src/ --out-dir lib/ -s",
    "es6watch": "babel --modules umd src/ --watch --out-dir lib/ -s",
    "test": "mocha --compilers js:babel-register",
    "lint": "eslint src"
  },
  "keywords": [
    "music",
    "notation",
    "score",
    "note"
  ],
  "author": "Sean Mullen <srmullen13@gmail.com> (http://seanmullen.me/)",
  "license": "Sean Mullen",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "chai": "^3.3.0",
    "jquery": "^2.1.4",
    "mocha": "^2.3.3",
    "palestrina.js": "^1.3.2"
  },
  "dependencies": {
    "fontloader": "^1.2.1",
    "fraction.js": "^3.0.0",
    "lodash": "^3.10.1",
    "teoria": "^1.12.0",
    "webfontloader": "^1.6.20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srmullen/boethius.git"
  }
}
